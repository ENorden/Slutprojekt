@model VeganRecipeVM[]
@{ ViewBag.Title = "Recipes";}

<h2>Recipe categories</h2>
@foreach (var item in Model)
{
    <div>
        <img src="@item.Img" style="width: 500px; height:auto" />
        <a asp-controller="Vegan" asp-action="Category" asp-route-id="@item.Id">@item.CategoryName</a>
    </div>
}






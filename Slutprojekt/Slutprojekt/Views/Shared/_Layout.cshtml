<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/StyleSheet.css")">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
</head>
<body>
    <header>
        <a asp-controller="Vegan" asp-action="recipes">Recipes</a>
        <a asp-controller="Vegan" asp-action="followers">Followers</a>
        <a asp-controller="Vegan" asp-action="postrecipe">Profile</a>
        <a asp-controller="Vegan" asp-action="logout">Logout</a>
    </header>
    <div>
        @RenderBody()
    </div>
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>
    @RenderSection("scripts", false)
</body>
</html>
